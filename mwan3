config interface 'wg0'
    option enabled '1'
    option reliability '1'
    option count '1'
    option timeout '2'
    option interval '5'
    option down '3'
    option up '8'
    list track_ip '8.8.8.8'
    list track_ip '8.8.4.4'
    option family 'ipv4'

config interface 'tunplus'
    option enabled '1'
    option reliability '1'
    option count '1'
    option timeout '2'
    option interval '5'
    option down '3'
    option up '8'
    list track_ip '8.8.8.8'
    list track_ip '8.8.4.4'
    option family 'ipv4'

config member 'wg0_m1_w1'
    option interface 'wg0'
    option metric '1'
    option weight '1'

config member 'tunplus_m1_w1'
    option interface 'tunplus'
    option metric '1'
    option weight '1'

config policy 'balanced'
    list use_member 'wg0_m1_w1'
    list use_member 'tunplus_m1_w1'
    option last_resort 'unreachable'

config rule 'default_rule'
    option dest_ip '0.0.0.0/0'
    option use_policy 'balanced'
